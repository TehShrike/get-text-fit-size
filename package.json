{
  "name": "get-text-fit-size",
  "version": "1.0.0",
  "description": "Find out what font size will make a text block fit inside of another element without wrapping",
  "main": "index.js",
  "scripts": {
    "test": "browserify test.js | tape-run",
    "test:safari": "npm t -- --browser=safari",
    "test:chrome": "npm t -- --browser=chrome",
    "test:firefox": "npm t -- --browser=firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/get-text-fit-size.git"
  },
  "keywords": [
    "text",
    "fit",
    "size",
    "width",
    "font"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/get-text-fit-size/issues"
  },
  "homepage": "https://github.com/TehShrike/get-text-fit-size#readme",
  "devDependencies": {
    "browserify": "14.4.0",
    "tape": "4.6.3",
    "tape-catch": "1.0.6",
    "tape-run": "3.0.0"
  }
}
